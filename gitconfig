# Let wezterm override the SSH rewrite rule. Necessary for wezterm's plugin system to work.
[includeIf "gitdir:~/Library/Application Support/wezterm/plugins/"]
    path = ~/.config/wezterm/.gitconfig
[includeIf "gitdir:~/.local/share/wezterm"]
    path = ~/.config/wezterm/.gitconfig

[url "ssh://git@github.com/"]
	insteadOf = "https://github.com/"

[user]
	name = Randall Fulton
	email = randall@shipt.com

[diff]
    external = difft
