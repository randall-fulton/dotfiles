shift+alt - f     : yabai -m window --toggle float; yabai -m window --grid 7:5:1:1:3:5
shift+alt - p     : yabai -m window --toggle zoom-fullscreen
shift+alt - h     : yabai -m window --swap west
shift+alt - j     : yabai -m window --swap south
shift+alt - k     : yabai -m window --swap north
shift+alt - l     : yabai -m window --swap east
shift+alt - y     : yabai -m window --focus west
shift+alt - u     : yabai -m window --focus south
shift+alt - i     : yabai -m window --focus north
shift+alt - o     : yabai -m window --focus east
shift+alt - b     : yabai -m space --balance
shift+alt - t     : yabai -m space --toggle padding; yabai -m space --toggle gap
shift+alt - s     : yabai -m window --toggle split
shift+alt - left  : yabai -m window --resize left:-100:0
shift+alt - down  : yabai -m window --resize bottom:0:100
shift+alt - up    : yabai -m window --resize bottom:0:-100
shift+alt - right : yabai -m window --resize left:100:0
